./Lab_1.out 10000 0.50 0.25 0.25 4 M
./Lab_1.out 10000 0.50 0.25 0.25 4 R
echo "    8 threads"
./Lab_1.out 10000 0.50 0.25 0.25 8 M
./Lab_1.out 10000 0.50 0.25 0.25 8 R