#!/bin/bash
gcc Lab_1.c LinkedList.c LinkedList.h -lm -pthread -o Lab_1.out
echo "9900"
./Lab_1.out 10000 0.99 0.005 0.005 1 S
./Lab_1.out 10000 0.99 0.005 0.005 1 M
./Lab_1.out 10000 0.99 0.005 0.005 1 R
echo "    2 threads"
./Lab_1.out 10000 0.99 0.005 0.005 2 M
./Lab_1.out 10000 0.99 0.005 0.005 2 R
echo "    4 threads"
./Lab_1.out 10000 0.99 0.005 0.005 4 M
./Lab_1.out 10000 0.99 0.005 0.005 4 R
echo "    8 threads"
./Lab_1.out 10000 0.99 0.005 0.005 8 M
./Lab_1.out 10000 0.99 0.005 0.005 8 R

echo "9000"
./Lab_1.out 10000 0.90 0.05 0.05 1 S
./Lab_1.out 10000 0.90 0.05 0.05 1 M
./Lab_1.out 10000 0.90 0.05 0.05 1 R
echo "    2 threads"
./Lab_1.out 10000 0.90 0.05 0.05 2 M
./Lab_1.out 10000 0.90 0.05 0.05 2 R
echo "    4 threads"
./Lab_1.out 10000 0.90 0.05 0.05 4 M
./Lab_1.out 10000 0.90 0.05 0.05 4 R
echo "    8 threads"
./Lab_1.out 10000 0.90 0.05 0.05 8 M
./Lab_1.out 10000 0.90 0.05 0.05 8 R

echo "5000"
./Lab_1.out 10000 0.50 0.25 0.25 1 S
./Lab_1.out 10000 0.50 0.25 0.25 1 M
./Lab_1.out 10000 0.50 0.25 0.25 1 R
echo "    2 threads"
./Lab_1.out 10000 0.50 0.25 0.25 2 M
./Lab_1.out 10000 0.50 0.25 0.25 2 R
